function rotateArrayfun(){
var arrayval = new Array();
arrayval = prompt("enter array elements separated by ,","1,2,3,4" );
arrayval = arrayval.split(",");
var arr = arrayval;
var num = document.form1.numofrotations.value;

for (var i = 0; i < num; i++) {
for(var j = 0; j < arrayval.length; j++) {
    var temp = arrayval[j+1];
    arrayval[j+1] = arrayval[0];
    arrayval[0] = temp;
}
}
   

alert("array elements before rotating"+ arr+ "and after rotating" + arrayval);
}

