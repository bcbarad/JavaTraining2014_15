function drawCicrle() {
var c = document.getElementById("myCanvas");
var ctx = c.getContext("2d");
ctx.beginPath();
ctx.arc(100, 75, 50, 0, 2 * Math.PI);
ctx.strokeStyle="green";
ctx.stroke();

ctx.beginPath();
ctx.arc(100,75,40,0,2*Math.PI);
ctx.strokeStyle = "purple"
ctx.stroke();

ctx.beginPath();
ctx.arc(100,75,30,0,2*Math.PI);
ctx.strokeStyle ="red";
ctx.stroke();
}
