

select finaclefun0_.FUND_CODE as FUND_COD1_2_, finaclefun0_.FUND_TRAN_TYPE as FUND_TRA2_2_, finaclefun0_.DAYEND_NAV as DAYEND_N3_2_, finaclefun0_.DAYEND_NAV_DATE as DAYEND_N4_2_, finaclefun0_.DIVIDEND_OPTION as DIVIDEND5_2_, finaclefun0_.EFFECTIVE_DATE as EFFECTIV6_2_, finaclefun0_.FUND_CATEGORY as FUND_CAT7_2_, finaclefun0_.FUND_ENABLED_IN_DIGI as FUND_ENA8_2_, finaclefun0_.FUND_HOUSE_ACCOUNT_NUMBER as FUND_HOU9_2_, finaclefun0_.FUND_HOUSE_ID as FUND_HO10_2_, finaclefun0_.FUND_HOUSE_NAME as FUND_HO11_2_, finaclefun0_.FUND_MASTER_DELETE as FUND_MA12_2_, finaclefun0_.FUND_NAME as FUND_NA13_2_, finaclefun0_.FUND_TRANS_TYPE_DELETE as FUND_TR14_2_, finaclefun0_.IPO_END_DATE as IPO_END15_2_, finaclefun0_.IPO_START_DATE as IPO_STA16_2_, finaclefun0_.ISIN as ISIN17_2_, finaclefun0_.SIP_ENABLED_IN_DIGI as SIP_ENA18_2_, finaclefun0_.TRAN_ENABLED_IN_DIGI as TRAN_EN19_2_ 
from FINACLE_FUND_DETAILS finaclefun0_ 
where (finaclefun0_.FUND_TRAN_TYPE in ('IPO Subscription')) 
and (finaclefun0_.TRAN_ENABLED_IN_DIGI in ('Y')) 
and (finaclefun0_.FUND_ENABLED_IN_DIGI in ('Y')) 
and (finaclefun0_.FUND_NAME in ('C')) 
-- and (finaclefun0_.FUND_HOUSE_ACCOUNT_NUMBER is not null) 

order by finaclefun0_.FUND_NAME asc limit 10;

select case when (finaclefun0_.IPO_START_DATE is not null and finaclefun0_.IPO_END_DATE is not null ) 
then (


select finaclefun0_.FUND_CODE as FUND_COD1_2_, finaclefun0_.FUND_TRAN_TYPE as FUND_TRA2_2_, finaclefun0_.DAYEND_NAV as DAYEND_N3_2_, finaclefun0_.DAYEND_NAV_DATE as DAYEND_N4_2_, finaclefun0_.DIVIDEND_OPTION as DIVIDEND5_2_, finaclefun0_.EFFECTIVE_DATE as EFFECTIV6_2_, finaclefun0_.FUND_CATEGORY as FUND_CAT7_2_, finaclefun0_.FUND_ENABLED_IN_DIGI as FUND_ENA8_2_, finaclefun0_.FUND_HOUSE_ACCOUNT_NUMBER as FUND_HOU9_2_, finaclefun0_.FUND_HOUSE_ID as FUND_HO10_2_, finaclefun0_.FUND_HOUSE_NAME as FUND_HO11_2_, finaclefun0_.FUND_MASTER_DELETE as FUND_MA12_2_, finaclefun0_.FUND_NAME as FUND_NA13_2_, finaclefun0_.FUND_TRANS_TYPE_DELETE as FUND_TR14_2_, finaclefun0_.IPO_END_DATE as IPO_END15_2_, finaclefun0_.IPO_START_DATE as IPO_STA16_2_, finaclefun0_.ISIN as ISIN17_2_, finaclefun0_.SIP_ENABLED_IN_DIGI as SIP_ENA18_2_, finaclefun0_.TRAN_ENABLED_IN_DIGI as TRAN_EN19_2_ 
from FINACLE_FUND_DETAILS finaclefun0_ 
where (finaclefun0_.FUND_TRAN_TYPE in ('IPO Subscription')) 
and (finaclefun0_.TRAN_ENABLED_IN_DIGI in ('Y')) 
and (finaclefun0_.FUND_ENABLED_IN_DIGI in ('Y')) 
and (finaclefun0_.FUND_NAME in ('C'))

and finaclefun0_.IPO_START_DATE is not null 
and finaclefun0_.IPO_END_DATE is not null

order by finaclefun0_.FUND_NAME asc limit 10;

select * from FINACLE_FUND_DETAILS finaclefun0_ 
where  finaclefun0_.IPO_START_DATE is not null 
and finaclefun0_.IPO_END_DATE is not null
 and current_date() between CAST(finaclefun0_.IPO_START_DATE AS DATE) AND CAST(finaclefun0_.IPO_END_DATE AS DATE) 
 and CURRENT_TIME < CAST('13:00:00' as TIME)

select now();

select CAST('13:00:00' as TIME);


